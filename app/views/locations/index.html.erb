<br />
<div class="CSSTableGenerator">
<table>
    <tr>
      <td>Temp</td>
      <td>City</td>
      <td>State</td>
      <td>Zip</td>
      <td>Lat</td>
      <td>Lon</td>
      <td></td>
      <td></td>
      <td></td>
    </tr>

    <% @locations.each do |location| %>
      <% wInfo = GetWeather.new(location.zip) %>
      <tr>
        <td><%= wInfo.temp_f %></td>
        <td><%= wInfo.city %></td>
        <td><%= wInfo.state %></td>
        <td><%= location.zip %></td>
        <td><%= wInfo.lat.to_f.round(4) %></td>
        <td><%= wInfo.lon.to_f.round(4) %></td>
        <td><%= link_to 'Show', location %></td>
        <td><%= link_to 'Edit', edit_location_path(location) %></td>
        <td><%= link_to 'Destroy', location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</table>
</div>
<br>

<%= link_to 'New Location', new_location_path %>
